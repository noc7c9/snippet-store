{
    "name": "@snippet-store/server",
    "version": "0.2.0",
    "repository": "https://github.com/noc7c9/snippet-store",
    "author": "Athir Saleem <noc7c9@gmail.com>",
    "license": "MIT",
    "private": true,
    "engines": {
        "node": "12.18.x"
    },
    "scripts": {
        "build": "yarn sucrase && rm -f build.zip && mv node_modules node_modules-dev && yarn install --production && zip -9r build.zip node_modules/ build/ && rm -rf build/; rm -rf node_modules && mv node_modules-dev node_modules",
        "sucrase": "rm -rf build/ && sucrase ./src --out-dir ./build --transforms typescript,imports",
        "start": "NODE_ENV=development watchexec --clear --restart --watch src/ ts-node src/local.ts"
    },
    "dependencies": {
        "@aws-sdk/client-dynamodb": "^1.0.0-gamma.6",
        "@snippet-store/common": "link:../common",
        "aws-serverless-express": "^3.3.8",
        "base-x": "^3.0.8",
        "dotenv-safe": "^8.2.0",
        "express": "^4.17.1",
        "morgan": "^1.10.0",
        "uuid": "^8.3.0"
    },
    "devDependencies": {
        "@types/aws-serverless-express": "^3.3.3",
        "@types/cors": "^2.8.7",
        "@types/dotenv-safe": "^8.1.1",
        "@types/express": "^4.17.7",
        "@types/morgan": "^1.9.1",
        "@types/node": "^14.6.2",
        "@types/uuid": "^8.3.0",
        "cors": "^2.8.5",
        "lorem-ipsum": "^2.0.3",
        "sucrase": "^3.15.0",
        "ts-node": "^9.0.0",
        "typescript": "^4.0.2"
    }
}
