    "server": "nodemon server.js"
  },
  "dependencies": {
    "autoprefixer": "^8.2.0",
    "body-parser": "^1.18.2",
    "browser-sync": "^2.23.6",
    "browser-sync-webpack-plugin": "^2.2.2",
    "bulma": "^0.6.2",
    "bulma-tagsinput": "^1.0.7",
    "clean-webpack-plugin": "^0.1.19",
    "clipboard-polyfill": "^2.4.6",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "eslint": "^4.19.1",
    "expose-loader": "^0.7.5",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "fuse.js": "^3.2.0",
    "jquery": "^3.3.1",
    "mkdirp": "^0.5.1",
    "mongodb": "^3.0.6",
    "node-sass": "^4.8.3",
    "nodemon": "^1.17.3",
    "postcss-loader": "^2.1.3",
    "pug": "^2.0.3",
    "pug-loader": "^2.4.0",
    "sass-lint": "^1.12.1",
    "sass-loader": "^6.0.7",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14"
  },
  "browserslist": [
    "last 2 versions"
  ],
